<% provide(:title, "Slack Aggregator") %>
<div class="row">
  <div class="col-lg-12">
    <h1 class="page-header">
      <i class="fa fa-file"></i> Links posted on your Slack Channels
    </h1>
  </div>
</div>
<% slack_links.each do |link| %>
  <pre>
    <pre>
    <%= link_to( link["text"], link["permalink"]) %>
      <p>Posted by ; <%= link['username']%>, in <%= link["channel"]["name"]%>, On: <%= DateTime.strptime((link["ts"]),"%s")%> </p>
  </pre>
  </pre>
<% end %>
